<?xml version="1.0"?>

<perfdatasources>
	<perfdatasource id="pds1">
		<host>host1.example.com</host> <service>Interface eth0</service>
		<name>ineth0</name>
		<type>GAUGE</type>
		<label>Données en entrée sur eth0</label>
		<factor>0.0</factor>
	</perfdatasource>
	
	<perfdatasource id="pds2">
		<host>host1.example.com</host> <service>Interface eth0</service>
		<name>outeth0</name>
		<type>GAUGE</type>
		<label>Données en sortie sur eth0</label>
	</perfdatasource>
	
	<perfdatasource id="pds3">
		<host>host1.example.com</host> <service>Interface eth1</service>
		<name>outeth1</name>
		<type>GAUGE</type>
		<label>Données en sortie sur eth1</label>
	</perfdatasource>
</perfdatasources>

<graphes>
	<graph>
		<name>graph1</name>
		<vlabel>Graph1</vlabel>
    	<group name="graphgroup1"/>
	</graph>

	<graph>
		<name>graph2</name>
		<vlabel>Graph2</vlabel>
    	
    	<perfdatasource id="pds1"/>
    	<perfdatasource id="pds2"/>
    	<perfdatasource id="pds3"/>
	</graph>

	<graph>
		<name>graph3</name>
		<vlabel>Graph3</vlabel>
    	
    	<perfdatasource id="pds3"/>
	</graph>

	<graph>
		<name>graph4</name>
		<vlabel>Graph4</vlabel>
	</graph>

	<graph>
		<name>graph5</name>
		<vlabel>Graph5</vlabel>
	</graph>
</graphes>

