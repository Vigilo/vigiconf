<?xml version="1.0"?>
<templates>
<template name="linux">
    <attribute name="snmpOIDsPerPDU">10</attribute>
    <group>Linux servers</group>
    <test name="all.Interface">
        <arg name="label">Loopback</arg>
        <arg name="ifname">lo</arg>
    </test>
    <test name="all.Partition">
        <arg name="label">Root</arg>
        <arg name="partname">/</arg>
    </test>
    <test name="all.TotalProcesses"/>
    <test name="all.Swap"/>
    <test name="all.Ping"/>
    <test name="all.NTPSync"/>
    <test name="all.Users"/>
    <test name="all.SSH"/>
    <test name="all.TCPConn"/>
    <test name="ucd.InterrCS"/>
    <test name="ucd.DiskIO"/>
    <test name="ucd.RAM"/>
    <test name="ucd.Load"/>
    <test name="linux.CPU"/>
    <test name="linux.RAID"/>
</template>
</templates>
