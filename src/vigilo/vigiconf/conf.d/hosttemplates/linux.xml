<?xml version="1.0"?>
<templates>
<template name="linux">
    <class>linux</class>
    <class>ucd</class>
    <attribute name="snmpOIDsPerPDU">10</attribute>
    <group>Linux servers</group>
    <test name="Interface">
        <arg name="label">Loopback</arg>
        <arg name="ifname">lo</arg>
    </test>
    <test name="Partition">
        <arg name="label">Root</arg>
        <arg name="partname">/</arg>
    </test>
    <test name="CPU"/>
    <test name="DiskIO"/>
    <test name="TotalProcesses"/>
    <test name="Swap"/>
    <test name="Ping"/>
    <test name="RAM"/>
    <test name="Load"/>
    <test name="NTPSync"/>
    <test name="Users"/>
    <test name="SSH"/>
    <test name="InterrCS"/>
    <test name="TCPConn"/>
    <test name="RAID"/>
</template>
</templates>
