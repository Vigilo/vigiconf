#, python-format
msgid ""
"The \"%(old_attribute)s\" attribute has been deprecated. Please use "
"\"%(replacement)s\" instead."
msgstr ""
"L'attribut \"%(old_attribute)s\" est obsolète. Veuillez utiliser "
"l'attribut \"%(replacement)s\" à la place."

#: src/vigilo/vigiconf/lib/confclasses/host.py:144
#, python-format
msgid "In host %(host)s: attribute name %(attribute)s is reserved"
msgstr "Hôte %(host)s : le nom d'attribut %(attribute)s est réservé"

#: src/vigilo/vigiconf/lib/confclasses/host.py:191
#, python-format
msgid ""
"Test \"%(test_name)s\" on \"%(host)s\" needs the following arguments: "
"%(args)s (and only those)"
msgstr ""
"Le test \"%(test_name)s\" sur \"%(host)s\" nécessite les arguments "
"suivants: %(args)s (et uniquement ceux-là)"

#: src/vigilo/vigiconf/lib/confclasses/host.py:516
#, python-format
msgid "host '%(host)s': wrong datasource in graph '%(graph)s': %(ds)s"
msgstr ""
"hôte '%(host)s' : source de données incorrecte sur le graphe '%(graph)s' "
": %(ds)s"

#: src/vigilo/vigiconf/lib/confclasses/host.py:540
#, python-format
msgid "CDEF name must be ASCII-only: %s"
msgstr "le nom CDEF doit être en ASCII pur: %s"

#: src/vigilo/vigiconf/lib/confclasses/host.py:729
msgid "Cannot override general configuration."
msgstr "Impossible d'écraser la configuration générale."

#: src/vigilo/vigiconf/lib/confclasses/host.py:739
msgid "oxe_login and oxe_password cannot be empty"
msgstr "oxe_login et oxe_password ne peuvent pas être vides"

#: src/vigilo/vigiconf/lib/confclasses/host.py:742
#, python-format
msgid ""
"Value for '%(attr)s' (%(value)s) could not be cast into an integer. Using"
" default value (%(default)d)."
msgstr ""
"La valeur de l'attibut '%(attr)s' (%(value)s) n'a pas pu être convertie "
